{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023"],
    "rootDir": "./",
    "outDir": "./dist",
    "module": "CommonJS", // Changed from ESNext
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "server.ts", "app.ts"],
  "exclude": ["node_modules"]
}
